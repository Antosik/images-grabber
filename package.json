{
  "name": "images-grabber",
  "description": "Grab images from some image services",
  "version": "3.1.4",
  "author": {
    "name": "Antosik"
  },
  "preferGlobal": true,
  "bin": {
    "images-grabber": "./bin/run",
    "img-grab": "./bin/run"
  },
  "main": "lib/index.js",
  "homepage": "https://github.com/Antosik/images-grabber",
  "bugs": "https://github.com/Antosik/images-grabber/issues",
  "license": "MIT",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-help": "^3.1.0",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "co": "^4.6.0",
    "listr": "^0.14.3",
    "lodash.flattendeep": "^4.4.0",
    "pixiv-app-api": "^1.2.1",
    "pixiv-img": "^1.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/tslint": "^3.1.1",
    "@types/jest": "^24.0.18",
    "@types/listr": "^0.14.2",
    "@types/node": "^10.12.18",
    "@types/xml2js": "^0.4.3",
    "globby": "^11.0.1",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "images-grabber",
    "commands": "./lib/commands",
    "hooks": {
      "init": "./lib/hooks/init",
      "command_not_found": "./lib/hooks/command_not_found"
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Antosik/images-grabber",
  "scripts": {
    "build": "rimraf lib && tsc",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "oclif-dev manifest",
    "test": "jest --runInBand"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "types": "lib/index.d.ts"
}
