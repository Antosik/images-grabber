{
  "name": "images-grabber",
  "description": "Grab images from some image services",
  "version": "3.0.0",
  "author": {
    "name": "Antosik"
  },
  "preferGlobal": true,
  "bin": {
    "images-grabber": "./bin/run",
    "img-grab": "./bin/run"
  },
  "main": "lib/index.js",
  "homepage": "https://github.com/Antosik/images-grabber",
  "bugs": "https://github.com/Antosik/images-grabber/issues",
  "license": "MIT",
  "dependencies": {
    "@oclif/command": "^1.5.8",
    "@oclif/config": "^1.10.4",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-help": "^2.1.4",
    "bignumber.js": "^8.0.1",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "co": "^4.6.0",
    "got": "^9.5.0",
    "listr": "^0.14.3",
    "lodash.flattendeep": "^4.4.0",
    "pixiv-app-api": "^0.11.1",
    "pixiv-img": "^0.3.1",
    "tslib": "^1.9.3",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.0",
    "@oclif/test": "^1.2.2",
    "@oclif/tslint": "^3.1.1",
    "@types/got": "^9.3.0",
    "@types/inquirer": "0.0.43",
    "@types/listr": "^0.13.0",
    "@types/minimist": "^1.2.0",
    "@types/node": "^10.12.18",
    "@types/xml2js": "^0.4.3",
    "globby": "^8.0.1",
    "lec": "^1.0.1",
    "rimraf": "^2.6.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/oclif.manifest.json",
    "lib"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "images-grabber",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Antosik/images-grabber",
  "scripts": {
    "build": "rimraf lib && tsc -b",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
