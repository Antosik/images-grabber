{
  "name": "images-grabber",
  "description": "Grab images from some image services",
  "version": "3.1.3",
  "author": {
    "name": "Antosik"
  },
  "preferGlobal": true,
  "bin": {
    "images-grabber": "./bin/run",
    "img-grab": "./bin/run"
  },
  "main": "lib/index.js",
  "homepage": "https://github.com/Antosik/images-grabber",
  "bugs": "https://github.com/Antosik/images-grabber/issues",
  "license": "MIT",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-help": "^2.2.1",
    "bignumber.js": "^9.0.0",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.3",
    "co": "^4.6.0",
    "got": "^9.6.0",
    "listr": "^0.14.3",
    "lodash.flattendeep": "^4.4.0",
    "pixiv-app-api": "^1.0.4",
    "pixiv-img": "^1.0.0",
    "tslib": "^1.10.0",
    "xml2js": "^0.4.22",
    "snyk": "^1.317.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/tslint": "^3.1.1",
    "@types/got": "^9.6.7",
    "@types/jest": "^24.0.18",
    "@types/listr": "^0.13.0",
    "@types/node": "^10.12.18",
    "@types/xml2js": "^0.4.3",
    "globby": "^10.0.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "images-grabber",
    "commands": "./lib/commands",
    "hooks": {
      "init": "./lib/hooks/init",
      "command_not_found": "./lib/hooks/command_not_found"
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Antosik/images-grabber",
  "scripts": {
    "build": "rimraf lib && tsc",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "oclif-dev manifest",
    "test": "jest --runInBand",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "types": "lib/index.d.ts",
  "snyk": true
}
