{
  "parser": "typescript-eslint-parser",
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "semi": [ 2, "always" ],
    "no-unexpected-multiline": 2,
    "linebreak-style": [ 2, "windows" ],
    "import/no-unresolved": [ "error", { "ignore": [ "src/" ] } ],
    "no-console":0
  }
}